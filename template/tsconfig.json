{
	"compilerOptions": {
		"target": "esnext",
		"module": "commonjs",
		"lib": ["es6"],
		"allowJs": true,
		"jsx": "react-native",
		"incremental": true,
		"strict": true,
		"moduleResolution": "node",
		"baseUrl": "./",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true
	},
	"exclude": ["node_modules", "babel.config.js", "metro.config.js", "jest.config.js"]
}
